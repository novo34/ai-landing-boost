{
  "name": "@ai-landing-boost/api",
  "version": "0.1.0",
  "private": true,
  "description": "NestJS API for AI Landing Boost SaaS",
  "scripts": {
    "build": "nest build && node -e \"const fs = require('fs'); const path = require('path'); const distMain = path.join(__dirname, 'dist', 'main.js'); const distSrcMain = path.join(__dirname, 'dist', 'src', 'main.js'); if (fs.existsSync(distSrcMain) && !fs.existsSync(distMain)) { try { fs.linkSync(distSrcMain, distMain); } catch(e) { fs.copyFileSync(distSrcMain, distMain); } }\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:validate": "prisma validate",
    "prisma:reset": "prisma migrate reset",
    "postinstall": "prisma generate",
    "prebuild": "prisma generate",
    "create-users": "ts-node -r tsconfig-paths/register scripts/create-users.ts",
    "migrate-crypto-legacy": "ts-node -r tsconfig-paths/register scripts/migrate-crypto-legacy.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^5.22.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/nodemailer": "^6.4.21",
    "@types/passport-google-oauth20": "^2.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-microsoft": "^1.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "googleapis": "^168.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "langdetect": "^0.2.1",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "openai": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-microsoft": "^0.0.1",
    "pdf-parse": "^2.4.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.19.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.18.2",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testMatch": [
      "**/src/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/../node_modules",
      "<rootDir>/../../node_modules"
    ]
  }
}
